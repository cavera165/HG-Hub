            });
            --<>----<>----<>----<>----<>----<>----<>--
            
            CombatSection:CreateToggle({
               Title = "Aimbot",
               Order = 1,
               Default = false,
               Callback = function(state)
                  getgenv().Aimbot = state;
                  if not getgenv().Aimbot then
                     target = nil;
                  end;
               end;
            });
            
            CombatSection:CreateToggle({
               Title = "Circle",
